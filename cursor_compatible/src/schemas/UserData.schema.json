{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserData",
  "type": "object",
  "properties": {
    "type": { "type": "string", "enum": ["profile", "api_key", "permission", "audit_log"] },
    "user_id": { "type": "string" },
    "timestamp": { "type": "integer" },
    "payload": { "type": "object" }
  },
  "required": ["type", "user_id", "timestamp", "payload"],
  "additionalProperties": false
} 