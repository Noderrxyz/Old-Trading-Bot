{
  "name": "@noderr/execution-optimizer",
  "version": "1.0.0",
  "description": "World-class smart order routing and execution engine for Noderr Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "ethers": "^5.7.2",
    "ccxt": "^4.1.22",
    "@flashbots/ethers-provider-bundle": "^0.6.2",
    "mathjs": "^11.11.0",
    "simple-statistics": "^7.8.3",
    "events": "^3.3.0",
    "winston": "^3.10.0",
    "node-cache": "^5.1.2",
    "ws": "^8.14.2",
    "axios": "^1.6.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.202",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "trading",
    "execution",
    "smart-order-routing",
    "mev-protection",
    "twap",
    "vwap",
    "algo-trading",
    "liquidity",
    "crypto",
    "defi",
    "institutional"
  ],
  "author": "Noderr Protocol",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ]
  }
} 