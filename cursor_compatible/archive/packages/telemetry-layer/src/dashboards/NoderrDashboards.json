              "id": "organize",
              "options": {
                "excludeByName": {
                  "Time": true
                },
                "renameByName": {
                  "strategy": "Strategy",
                  "Value": "P&L (USD)"
                }
              }
            },
            {
              "id": "sortBy",
              "options": {
                "fields": {},
                "sort": [
                  {
                    "field": "P&L (USD)",
                    "desc": true
                  }
                ]
              }
            }
          ]
        }
      ]
    },
    {
      "uid": "noderr-comparison",
      "title": "Noderr Protocol - Backtest vs Paper vs Live",
      "description": "Performance comparison across different execution modes",
      "tags": ["noderr", "comparison", "analysis"],
      "refresh": "5s",
      "panels": [
        {
          "id": "pnl-overlay",
          "title": "P&L Curve Overlay",
          "type": "graph",
          "gridPos": { "x": 0, "y": 0, "w": 24, "h": 10 },
          "targets": [
            {
              "expr": "sum(noderr_pnl_24h{mode=\"live\"}) by (mode)",
              "legendFormat": "Live Trading",
              "refId": "A"
            },
            {
              "expr": "sum(noderr_pnl_24h{mode=\"paper\"}) by (mode)",
              "legendFormat": "Paper Trading",
              "refId": "B"
            },
            {
              "expr": "sum(noderr_pnl_24h{mode=\"backtest\"}) by (mode)",
              "legendFormat": "Backtest",
              "refId": "C"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "unit": "currencyUSD",
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "smooth",
                "lineWidth": 2,
                "fillOpacity": 10
              }
            }
          }
        },
        {
          "id": "consistency-score",
          "title": "Consistency Score",
          "type": "gauge",
          "gridPos": { "x": 0, "y": 10, "w": 8, "h": 6 },
          "targets": [
            {
              "expr": "100 - (abs(noderr_pnl_24h{mode=\"live\"} - noderr_pnl_24h{mode=\"backtest\"}) / noderr_pnl_24h{mode=\"backtest\"} * 100)",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "unit": "percent",
              "min": 0,
              "max": 100,
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "red", "value": 0 },
                  { "color": "yellow", "value": 80 },
                  { "color": "green", "value": 90 }
                ]
              }
            }
          }
        },
        {
          "id": "discrepancy-tracker",
          "title": "P&L Discrepancy",
          "type": "bar",
          "gridPos": { "x": 8, "y": 10, "w": 8, "h": 6 },
          "targets": [
            {
              "expr": "abs(noderr_pnl_24h{mode=\"live\"} - noderr_pnl_24h{mode=\"backtest\"})",
              "legendFormat": "Live vs Backtest",
              "refId": "A"
            },
            {
              "expr": "abs(noderr_pnl_24h{mode=\"paper\"} - noderr_pnl_24h{mode=\"backtest\"})",
              "legendFormat": "Paper vs Backtest",
              "refId": "B"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "unit": "currencyUSD",
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "green", "value": 0 },
                  { "color": "yellow", "value": 1000 },
                  { "color": "red", "value": 5000 }
                ]
              }
            }
          }
        },
        {
          "id": "volume-comparison",
          "title": "Trade Volume Comparison",
          "type": "stat",
          "gridPos": { "x": 16, "y": 10, "w": 8, "h": 6 },
          "targets": [
            {
              "expr": "sum(rate(noderr_requests_total{module=\"execution-optimizer\"}[1h])) by (mode)",
              "format": "table",
              "instant": true,
              "refId": "A"
            }
          ],
          "transformations": [
            {
              "id": "organize",
              "options": {
                "renameByName": {
                  "mode": "Mode",
                  "Value": "Trades/Hour"
                }
              }
            }
          ]
        }
      ]
    }
  ],
